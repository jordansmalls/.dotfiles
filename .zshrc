
# GENERAL
alias c="clear"
alias tree="tree -C"
alias l="ls -lah"
alias .="cd .."
alias ..="cd ../.."

# INFO
alias nf="neofetch"
alias ff="fastfetch"

# NVIM
alias n="nvim ."
alias vi="nvim"
alias vim="nvim"
alias nano="nano -w"


# NPM
alias ni="npm install"
alias ns="npm start"
alias nt="npm test"
alias nd="npm run dev"

# YARN
alias yi="yarn install"
alias ya="yarn add"
alias ys="yarn start"
alias yt="yarn test"
alias yd="yarn dev"

# PNPM
alias p="pnpm"
alias pa="pnpm add"
alias pi="pnpm install"
alias ps="pnpm start"
alias pd="pnpm dev"

alias zs="source ~/.zshrc"
